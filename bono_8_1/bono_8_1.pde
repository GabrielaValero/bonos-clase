//se define una variable con un numero especifico
//se evalua si ese numero es mayor o igual a 10
//si lo es, se imprime un mensaje indicando que cumple la condición
//si no lo es, se imprime otro mensaje indicando que no la cumple

void setup() {
  int numero = 20;  // aqui se ingresa el numero

  if (numero >= 10) {
    println("El número es mayor o igual a 10");
  } else {
    println("El número es menor que 10");
  }
}
